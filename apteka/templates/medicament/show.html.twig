{% extends 'base.html.twig' %}

{% block title %}Medicament{% endblock %}

{% block body %}
    <h1>Medicament</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ medicament.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ medicament.name }}</td>
            </tr>
            <tr>
                <th>DateOfProduced</th>
                <td>{{ medicament.dateOfProduced ? medicament.dateOfProduced|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Return</th>
                <td>{{ medicament.return }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_medicament_index') }}">back to list</a>

    <a href="{{ path('app_medicament_edit', {'id': medicament.id}) }}">edit</a>

    {{ include('medicament/_delete_form.html.twig') }}
{% endblock %}
